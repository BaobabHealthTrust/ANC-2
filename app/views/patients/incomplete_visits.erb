<style type="text/css">
  body {
    -moz-user-select:none;
  }

  #stats {
    width:98%;
    border:1px solid #eee;
    margin:10px;
    margin-top: 0px;
    background:white;
  }
  #stats td, #stats th {
    text-align:center;
  }
  #stats tr td:nth-child(1){
    text-align: left;
    padding-left: 2%;
  }
  .odd {
    background-color: #eeeef7;
  }
  .even {
    background-color: #fff;
  }

  #table_div {
    overflow:auto;
    height: 320px;
    background-color: white;
    border: solid 1px #eee;
    border-width: 1px;
    overflow: hidden;
  }

  .btn {
    min-height: 50px;
    margin: 0px;
    margin-bottom: 5px;
    font-size: 1.2em;
    background-color: #6281a7;
    color: #FFF;
    font-weight: normal;
    border: 5px outset #000;
    padding: 10px;
    cursor: pointer;
    background:url(../images/btn_blue.png) right repeat;
    -moz-user-select:none;
    -moz-user-focus:disabled;
  }

  .btn:hover {
    background:url(../images/btn_blue_hover.png) right repeat;
    -moz-user-select:none;
    -moz-user-focus:disabled;
  }

  .btn:active {
    background:url(../images/btn_yellow.png) right repeat;
    border: 3px inset #000;
  }

</style>
  <h2 style="text-align: center; color: #6281a7; margin-top: -2px; margin-bottom: 0px;">Incompete Visits</h2>
  <table id="stats" style="margin-bottom: 8px;" cellpadding="5">
    <tr style="color: #fff; background-color: #6281a7;">
      <th>Patient Name</th>
      <th>National ID</th>
      <th>Visit Number</th>
      <th>Visit Date</th>
    </tr>
    <%@incomplete_visits.each do |visit| %>
        <tr>
        <td><%=visit["name"]%></td>
        <td><%=visit["n_id"]%></td>
        <td><%=visit["visit_no"]%></td>
        <td><%=visit["visit_date"]%></td>
    </tr>
    <%end%>
  </table>